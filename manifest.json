{
  "name": "Github 1S opener",
  "description": "Adds a button to github to open the repository in Github1S",
  "version": "1.0",
  "manifest_version": 3,
  "author":"Aitor Garcia Diez",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "tab",
    "scripting",
    "webNavigation",
    "tabs"
  ],
  "host_permissions": [
    "https://github.com/*/*/"
  ],
  "icons": {
    "16": "/images/logo.png",
    "32": "/images/logo.png",
    "48": "/images/logo.png",
    "128": "/images/logo.png"
  }
}